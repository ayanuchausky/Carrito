<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Carrito.
    -->
    <changeSet id="20200607050100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="fecha_id"
                                 baseTableName="carrito"
                                 constraintName="fk_carrito_fecha_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fechas"/>

        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="carrito"
                                 constraintName="fk_carrito_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

    </changeSet>
</databaseChangeLog>
